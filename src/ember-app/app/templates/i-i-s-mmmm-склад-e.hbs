<h3 class="ui header">{{t "forms.i-i-s-mmmm-склад-e.caption"}}</h3>

<form class="ui form flexberry-vertical-form" role="form">
  {{flexberry-error error=error}}
  <div class="field">
    <div class="flexberry-edit-panel">
      {{#unless readonly}}
        <button class="ui button save-button" {{action "save"}}>
          {{t "forms.edit-form.save-button-text"}}
        </button>
        {{#unless model.isNew}}
          <button class="ui button save-del-button" {{action "delete"}}>
            {{t "forms.edit-form.delete-button-text"}}
          </button>
        {{/unless}}
      {{/unless}}
      <button class="ui button close-button" {{action "close"}}>
        {{t "forms.edit-form.close-button-text"}}
      </button>
    </div>
  </div>
  <div class="field flexberry-validationsummary-container">
    <div class="sixteen wide">
      {{flexberry-validationsummary errors=(v-get validationObject "messages")}}
    </div>
  </div>
  <div class="field">
    {{flexberry-field
      readonly=readonly
      value=model.кодСклада
      class=(if (v-get validationModel "кодСклада" "isInvalid") "error")
      label=(t "forms.i-i-s-mmmm-склад-e.кодСклада-caption")
      data-test-i-i-s-mmmm-склад-e-кодСклада=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "кодСклада" "message")}}
  </div>
  <div class="field">
    {{flexberry-field
      readonly=readonly
      value=model.имяСклада
      class=(if (v-get validationModel "имяСклада" "isInvalid") "error")
      label=(t "forms.i-i-s-mmmm-склад-e.имяСклада-caption")
      data-test-i-i-s-mmmm-склад-e-имяСклада=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "имяСклада" "message")}}
  </div>
  <div class="field">
    <label>{{t "forms.i-i-s-mmmm-склад-e.тЧАктаПриемки-caption"}}</label>
    {{flexberry-lookup
      choose="showLookupDialog"
      remove="removeLookupValue"
      value=model.тЧАктаПриемки
      displayAttributeName="номерПозиции"
      autocomplete=true
      relationName="тЧАктаПриемки"
      projection="ТЧАктаПриемкиL"
      title=(t "forms.i-i-s-mmmm-склад-e.тЧАктаПриемки-caption")
      readonly=readonly
      componentName="тЧАктаПриемкиLookup"
      class=(if (v-get validationModel "тЧАктаПриемки" "isInvalid") "error")
      data-test-i-i-s-mmmm-склад-e-тЧАктаПриемки=true
    }}
    {{flexberry-validationmessage error=(v-get validationModel "тЧАктаПриемки" "message")}}
  </div>
</form>
